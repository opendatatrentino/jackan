<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-Integration Test</actionName>
            <displayName>Integration Test</displayName>
            <goals>
                <goal>integration-test</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Deploy to Sonatype</actionName>
            <displayName>Deploy to Sonatype</displayName>
            <goals>
                <goal>clean</goal>
                <goal>deploy</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Start sonar</actionName>
            <displayName>Start sonar</displayName>
            <goals>
                <goal>clean</goal>
                <goal>sonar:sonar</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
        <action>
            <actionName>build</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>install</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
        <action>
            <actionName>rebuild</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
    </actions>
